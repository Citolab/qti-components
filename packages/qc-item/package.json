{
    "name": "@qti-components/item",
    "version": "1.0.0",
    "description": "QTI item utilities",
    "main": "dist/index.js",
    "scripts": {
        "build": "wireit",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "qti",
        "item"
    ],
    "author": "",
    "license": "GPL-3.0-only",
    "dependencies": {
        "@qti-components/elements": "workspace:*",
        "@qti-components/interactions": "workspace:*",
        "@qti-components/processing": "workspace:*",
        "@qti-components/utilities": "workspace:*",
        "@qti-components/transformers": "workspace:*",
        "@qti-components/shared": "workspace:*"
    },
    "peerDependencies": {
        "lit": "^3.2.1"
    },
    "wireit": {
        "build": {
            "command": "tsc --pretty",
            "files": [
                "src/**/*.ts",
                "!src/**/*.stories.ts",
                "tsconfig.json"
            ],
            "output": [
                "index.{d.ts,d.ts.map,js,js.map}",
                "lib",
                "test"
            ],
            "clean": "if-file-deleted"
        }
    }
}