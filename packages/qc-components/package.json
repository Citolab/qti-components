{
    "name": "@qti-components/components",
    "version": "1.0.0",
    "description": "QTI component utilities",
    "main": "dist/index.js",
    "scripts": {
        "build": "wireit",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "qti",
        "components"
    ],
    "author": "",
    "type": "module",
    "types": "./dist/index.d.ts",
    "sideEffects": true,
    "license": "GPL-3.0-only",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./exports/*": {
            "import": "./dist/exports/*"
        },
        "./qti-components": {
            "types": "./dist/qti-components/index.d.ts",
            "import": "./dist/qti-components/index.js"
        },
        "./qti-item": {
            "types": "./dist/qti-item/index.d.ts",
            "import": "./dist/qti-item/index.js"
        },
        "./qti-test": {
            "types": "./dist/qti-test/index.d.ts",
            "import": "./dist/qti-test/index.js"
        },
        "./qti-loader": {
            "types": "./dist/qti-loader/index.d.ts",
            "import": "./dist/qti-loader/index.js"
        },
        "./qti-transformers": {
            "types": "./dist/qti-transformers/index.d.ts",
            "import": "./dist/qti-transformers/index.js"
        },
        "./customElements": "./custom-elements.json",
        "./react": "./dist/qti-components-jsx.d.ts",
        "./item.css": "./dist/item.css",
        "./cdn/*": "./cdn/*",
        "./package.json": "./package.json"
    },
    "files": [
        "dist",
        "cdn",
        "custom-elements.json"
    ],
    "dependencies": {
        "@qti-components/elements": "workspace:*",
        "@qti-components/test": "workspace:*",
        "@qti-components/item": "workspace:*",
        "@qti-components/transformers": "workspace:*"
    },
    "peerDependencies": {
        "@heximal/templates": "^0.1.5",
        "@lit/context": "^1.1.3",
        "lit": "^3.2.1"
    },
    "wireit": {
        "build": {
            "command": "tsc --pretty",
            "dependencies": [
                "../qc-elements:build",
                "../qc-test:build",
                "../qc-item:build",
                "../qc-transformers:build"
            ],
            "files": [
                "src/**/*.ts",
                "!src/**/*.stories.ts",
                "!src/**/*.test.ts",
                "!src/**/*.spec.ts",
                "tsconfig.json"
            ],
            "output": [
                "index.{d.ts,d.ts.map,js,js.map}",
                "components/**/*.{d.ts,d.ts.map,js,js.map}",
                "internal/**/*.{d.ts,d.ts.map,js,js.map}"
            ],
            "clean": "if-file-deleted"
        }
    }
}