{
    "name": "@qti-components/components",
    "version": "1.0.0",
    "description": "QTI component utilities",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsup",
        "clean": "rm -rf dist cdn",
        "attw": "attw --profile esm-only --pack",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "qti",
        "components"
    ],
    "author": "",
    "type": "module",
    "sideEffects": [
        "./dist/**/*.css"
    ],
    "license": "GPL-3.0-only",
    "exports": {
        ".": {
            "import": "./dist/index.js"
        },
        "./exports/*": {
            "import": "./dist/shared.js"
        },
        "./qti-elements": {
            "import": "./dist/elements.js"
        },
        "./qti-interactions": {
            "import": "./dist/interactions.js"
        },
        "./qti-item": {
            "import": "./dist/item.js"
        },
        "./qti-test": {
            "import": "./dist/test.js"
        },
        "./qti-loader": {
            "import": "./dist/loader.js"
        },
        "./qti-transformers": {
            "import": "./dist/transformers.js"
        },
        "./customElements": "./custom-elements.json",
        "./item.css": "./dist/item.css",
        "./cdn/*": "./cdn/*",
        "./package.json": "./package.json"
    },
    "files": [
        "dist",
        "cdn",
        "custom-elements.json"
    ],
    "dependencies": {
        "@qti-components/test": "workspace:*",
        "@qti-components/item": "workspace:*",
        "@qti-components/elements": "workspace:*",
        "@qti-components/transformers": "workspace:*",
        "@qti-components/interactions": "workspace:*",
        "@qti-components/loader": "workspace:*",
        "@qti-components/shared": "workspace:*"
    },
    "peerDependencies": {
        "lit": "^3.3.1",
        "@lit/context": "^1.1.6",
        "@heximal/templates": "^0.1.5"
    },
    "devDependencies": {
        "tsup": "^8.5.0"
    }
}