{
    "name": "@qti-components/components",
    "version": "1.0.0",
    "description": "QTI component utilities",
    "main": "dist/index.js",
    "scripts": {
        "build": "wireit",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "qti",
        "components"
    ],
    "author": "",
    "type": "module",
    "sideEffects": true,
    "license": "GPL-3.0-only",
    "exports": {
        ".": {
            "import": "./dist/index.js"
        },
        "./exports/*": {
            "import": "./dist/exports/*"
        },
        "./qti-components": {
            "import": "./dist/qti-components/index.js"
        },
        "./qti-item": {
            "import": "./dist/qti-item/index.js"
        },
        "./qti-test": {
            "import": "./dist/qti-test/index.js"
        },
        "./qti-loader": {
            "import": "./dist/qti-loader/index.js"
        },
        "./qti-transformers": {
            "import": "./dist/qti-transformers/index.js"
        },
        "./customElements": "./custom-elements.json",
        "./item.css": "./dist/item.css",
        "./cdn/*": "./cdn/*",
        "./package.json": "./package.json"
    },
    "files": [
        "dist",
        "cdn",
        "custom-elements.json"
    ],
    "dependencies": {
        "@qti-components/test": "workspace:*",
        "@qti-components/item": "workspace:*",
        "@qti-components/elements": "workspace:*",
        "@qti-components/transformers": "workspace:*",
        "@qti-components/interactions": "workspace:*",
        "@qti-components/loader": "workspace:*",
        "@qti-components/shared": "workspace:*"
    },
    "peerDependencies": {
        "@heximal/templates": "^0.1.5",
        "@lit/context": "^1.1.3",
        "lit": "^3.2.1"
    },
    "wireit": {
        "build": {
            "command": "tsup"
        }
    }
}