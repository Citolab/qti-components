{
    "name": "@qti-components/interactions",
    "version": "1.0.0",
    "description": "QTI interactions utilities",
    "main": "dist/index.js",
    "scripts": {
        "build": "wireit",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "qti",
        "interactions"
    ],
    "author": "",
    "license": "GPL-3.0-only",
    "dependencies": {
        "@qti-components/shared": "workspace:*",
        "@qti-components/transformers": "workspace:*",
        "@qti-components/utilities": "workspace:*"
    },
    "peerDependencies": {
        "lit": "^3.2.1"
    },
    "wireit": {
        "build": {
            "command": "tsc --pretty",
            "files": [
                "src/**/*.ts",
                "!src/**/*.test.ts",
                "!src/**/*.spec.ts",
                "!src/**/*.stories.ts",
                "tsconfig.json"
            ],
            "output": [
                "dist/*.{d.ts,d.ts.map,js,js.map}"
            ],
            "clean": "if-file-deleted"
        }
    }
}