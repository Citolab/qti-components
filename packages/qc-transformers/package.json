{
    "name": "@qti-components/transformers",
    "version": "1.0.0",
    "description": "QTI transformation utilities",
    "main": "dist/index.js",
    "scripts": {
        "build": "wireit",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "qti",
        "transformers"
    ],
    "author": "",
    "license": "GPL-3.0-only",
    "wireit": {
        "build": {
            "command": "tsc --pretty",
            "files": [
                "src/**/*.ts",
                "tsconfig.json"
            ],
            "output": [
                "dist/*.{d.ts,d.ts.map,js,js.map}"
            ],
            "clean": "if-file-deleted"
        }
    }
}