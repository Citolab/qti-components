{
  "name": "@citolab/qti-components",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Citolab/qti-components.git"
  },
  "description": "QTI webcomponents",
  "author": {
    "name": "Patrick de Klein"
  },
  "contributors": [
    {
      "name": "Sander Looise"
    },
    {
      "name": "Marcel Hoekstra"
    }
  ],
  "license": "GPLv3",
  "private": false,
  "engines": {
    "node": ">=20.0.0"
  },
  "version": "6.9.1-beta.50",
  "type": "module",
  "exports": {
    "./qti-components": "./dist/qti-components/index.js",
    "./qti-transformers": "./dist/qti-transformers/index.js",
    "./qti-item": "./dist/qti-item/index.js",
    "./qti-loader": "./dist/qti-loader/index.js",
    "./item.css": "./dist/item.css"
  },
  "types": "./dist/qti-components/index.d.ts",
  "typesVersions": {
    "*": {
      "qti-components": [
        "./dist/qti-components/index.d.ts"
      ],
      "qti-transformers": [
        "./dist/qti-transformers/index.d.ts"
      ],
      "qti-item": [
        "./dist/qti-item/index.d.ts"
      ],
      "qti-loader": [
        "./dist/qti-loader/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "types",
    "./dist/item.css"
  ],
  "scripts": {
    "test": "vitest",
    "dev": "vite",
    "build": "wireit",
    "build-lib": "tsx scripts/build.ts",
    "watch-lib": "nodemon --watch \"src/**\" --ext \"ts,json,css\" --ignore \"src/**/*.spec.ts\" --exec \"tsx scripts/build.ts\" watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook:build": "storybook build",
    "storybook:test": "test-storybook",
    "cem": "cem analyze --watch",
    "lint": "eslint src/lib/**",
    "arethetypeswrong": "wireit",
    "----hooks----": "-",
    "prepublishOnly": "echo 'calling prepublishOnly as npm lifecycle hook to build-lib!' && npm run build-lib",
    "postpublish": "echo 'posthook for prosperity, does nothing for now!'",
    "test:browser": "vitest --browser.headless=false",
    "chromatic": "node chromatic-runner.cjs"
  },
  "wireit": {
    "build": {
      "command": "tsc"
    },
    "arethetypeswrong": {
      "command": "attw --pack",
      "dependencies": [
        "build-lib"
      ]
    }
  },
  "peerDependencies": {
    "@lit/context": "^1.1.2",
    "lit": "^3.2.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@chromatic-com/storybook": "^3.2.2",
    "@custom-elements-manifest/analyzer": "^0.10.3",
    "@eslint/js": "^9.13.0",
    "@lit-labs/preact-signals": "^1.0.2",
    "@lit/context": "^1.1.3",
    "@storybook/addon-a11y": "^8.4.0",
    "@storybook/addon-essentials": "^8.4.0",
    "@storybook/addon-interactions": "^8.4.0",
    "@storybook/addon-links": "^8.4.0",
    "@storybook/blocks": "^8.4.0",
    "@storybook/preview-api": "^8.4.0",
    "@storybook/test": "^8.4.0",
    "@storybook/test-runner": "^0.19.1",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/web-components": "^8.4.0",
    "@storybook/web-components-vite": "^8.4.0",
    "@testing-library/dom": "^10.4.0",
    "@types/dom-view-transitions": "^1.0.5",
    "@types/node": "^22.8.6",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "@vitest/browser": "^2.1.4",
    "@wdio/cli": "^9.2.6",
    "autoprefixer": "^10.4.20",
    "chromatic": "^11.16.3",
    "custom-element-vs-code-integration": "^1.4.1",
    "dotenv": "^16.4.5",
    "eslint": "^9.13.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-lit": "^1.15.0",
    "eslint-plugin-lit-a11y": "^4.1.4",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-storybook": "^0.10.1",
    "eslint-plugin-wc": "^2.2.0",
    "globals": "^15.11.0",
    "lit": "^3.2.1",
    "nodemon": "^3.1.7",
    "np": "^10.0.7",
    "path-browserify": "^1.0.1",
    "playwright": "^1.48.2",
    "postcss": "^8.4.47",
    "postcss-class-apply": "^4.0.1",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^13.0.1",
    "prettier": "3.3.3",
    "remark-gfm": "^4.0.0",
    "storybook": "^8.4.0",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-prettier": "^5.0.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.12.2",
    "vite": "^5.4.10",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.4",
    "wireit": "^0.14.9"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.24.3"
  },
  "customElements": "custom-elements.json",
  "bugs": {
    "url": "https://github.com/Citolab/qti-components/issues"
  },
  "homepage": "https://github.com/Citolab/qti-components#readme"
}
