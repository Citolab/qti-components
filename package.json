{
  "name": "@citolab/qti-components",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Citolab/qti-components.git"
  },
  "description": "QTI webcomponents",
  "author": {
    "name": "Patrick de Klein"
  },
  "contributors": [
    {
      "name": "Sander Looise"
    },
    {
      "name": "Marcel Hoekstra"
    }
  ],
  "license": "GPLv3",
  "private": false,
  "engines": {
    "node": ">=20.0.0"
  },
  "version": "7.0.6-beta.6",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./exports/*": {
      "import": "./dist/exports/*"
    },
    "./qti-components": "./dist/qti-components/index.js",
    "./qti-test/core": "./dist/qti-test/core/index.js",
    "./qti-test/components/*": {
      "type": "./dist/qti-test/components/*.d.ts",
      "import": "./dist/qti-test/components/*"
    },
    "./qti-item/core": "./dist/qti-item/core/index.js",
    "./qti-item/components/*": {
      "type": "./dist/qti-item/components/*.d.ts",
      "import": "./dist/qti-item/components/*"
    },
    "./qti-loader": "./dist/qti-loader/index.js",
    "./qti-transformers": "./dist/qti-transformers/index.js",
    "./react": "./dist/qti-components-jsx.d.ts",
    "./item.css": "./dist/item.css"
  },
  "files": [
    "dist",
    "cdn"
  ],
  "scripts": {
    "storybook": "wireit",
    "storybook:docs": "wireit",
    "build:watch": "npm run yalc --watch",
    "npm:publish": "np",
    "---": "-",
    "build": "wireit",
    "test": "wireit",
    "yalc": "wireit",
    "attw": "wireit",
    "lint": "eslint src/**/*.ts",
    "cem": "wireit",
    "css": "wireit",
    "act": "act --container-architecture linux/arm64 -W .github/workflows/pkgr-release.yml",
    "vite": "vite",
    "stylelint": "stylelint \"src/**/*.css\"",
    "build-storybook": "storybook build",
    "test-storybook": "wireit",
    "storybook:docs:build": "wireit",
    "chromatic": "node chromatic-runner.cjs",
    "----hooks----": "-",
    "prepare": "husky"
  },
  "dependencies": {},
  "peerDependencies": {
    "@lit/context": "^1.1.3",
    "lit": "^3.2.1",
    "stampino": "^0.8.3"
  },
  "devDependencies": {
    "lit": "^3.2.1",
    "stampino": "^0.8.3",
    "@arethetypeswrong/cli": "^0.17.1",
    "@chromatic-com/storybook": "^3.2.2",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@custom-elements-manifest/analyzer": "^0.10.3",
    "@eslint/js": "^9.16.0",
    "@lit/context": "^1.1.3",
    "@storybook/addon-a11y": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/addon-storysource": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/preview-api": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@storybook/test-runner": "^0.21.0",
    "@storybook/web-components": "^8.4.7",
    "@storybook/web-components-vite": "^8.4.7",
    "@swc/core": "^1.10.1",
    "@testing-library/dom": "^10.4.0",
    "@types/dom-view-transitions": "^1.0.5",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitest/browser": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "globby": "^14.0.2",
    "cem-plugin-expanded-types": "^1.3.3",
    "chromatic": "^11.20.0",
    "custom-element-eslint-rule-generator": "^1.0.1",
    "custom-element-jsx-integration": "^1.5.4",
    "custom-element-react-wrappers": "^1.6.8",
    "custom-element-vs-code-integration": "^1.4.1",
    "dotenv": "^16.4.7",
    "eslint": "^9.16.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-lit": "^1.15.0",
    "eslint-plugin-lit-a11y": "^4.1.4",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-storybook": "^0.11.1",
    "eslint-plugin-wc": "^2.2.0",
    "fast-xml-parser": "^4.5.0",
    "globals": "^15.13.0",
    "husky": "^9.1.7",
    "np": "^10.1.0",
    "path-browserify": "^1.0.1",
    "pkg-pr-new": "^0.0.37",
    "playwright": "^1.49.0",
    "postcss": "^8.4.49",
    "postcss-class-apply": "^4.0.1",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^13.0.1",
    "prettier": "3.4.2",
    "remark-gfm": "^4.0.0",
    "shadow-dom-testing-library": "^1.11.3",
    "storybook": "^8.4.7",
    "storybook-addon-tag-badges": "^1.3.2",
    "stylelint": "^16.11.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-prettier": "^5.0.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.0",
    "vite": "^6.0.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8",
    "wc-storybook-helpers": "^2.0.4",
    "wireit": "^0.14.9"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.28.1"
  },
  "customElements": "custom-elements.json",
  "bugs": {
    "url": "https://github.com/Citolab/qti-components/issues"
  },
  "homepage": "https://github.com/Citolab/qti-components#readme",
  "wireit": {
    "storybook": {
      "command": "storybook dev -p 6006",
      "dependencies": [
        "storybook:cem:watch"
      ]
    },
    "yalc": {
      "command": "yalc push",
      "dependencies": [
        "build"
      ]
    },
    "build": {
      "command": "tsup",
      "dependencies": [
        "cem",
        "css"
      ],
      "files": [
        "src/**/*.ts",
        "tsconfig.json",
        "package.json",
        "tsup.config.ts"
      ]
    },
    "cem": {
      "command": "cem analyze"
    },
    "test-storybook": {
      "command": "test-storybook --excludeTags no-tests"
    },
    "css": {
      "command": "postcss src/item.css -d dist -m"
    },
    "test": {
      "command": "vitest run"
    },
    "storybook:docs": {
      "command": "storybook dev --docs -c .storybook-docs",
      "dependencies": [
        "storybook:cem:watch"
      ]
    },
    "storybook:docs:build": {
      "command": "storybook build --docs -c .storybook-docs --output-dir docs",
      "dependencies": [
        "cem"
      ]
    },
    "storybook:cem:watch": {
      "command": "cem analyze --watch",
      "service": {
        "readyWhen": {
          "lineMatches": "Created new manifest."
        }
      }
    },
    "attw": {
      "command": "attw --profile esm-only --pack --exclude-entrypoints  ./item.css ./react"
    }
  }
}
