{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": true,
    "experimentalDecorators": true,
    "importHelpers": false,
    "inlineSources": true,
    "lib": [
      "ES2022",
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "esnext",
    "moduleResolution": "bundler",
    "target": "esnext",
    "noImplicitAny": false,
    "noImplicitOverride": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "noUnusedLocals": false,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": false,
    "useDefineForClassFields": false,
    "paths": {
      "@citolab/qti-components": [
        "./packages/qti-components/src/index.ts"
      ],
      "@qti-components/base": [
        "./packages/qti-base/src/index.ts"
      ],
      "@qti-components/elements": [
        "./packages/qti-elements/src/index.ts"
      ],
      "@qti-components/interactions": [
        "./packages/qti-interactions/src/index.ts"
      ],
      "@qti-components/processing": [
        "./packages/qti-processing/src/index.ts"
      ],
      "@qti-components/item": [
        "./packages/qti-item/src/index.ts"
      ],
      "@qti-components/prosemirror": [
        "./packages/qti-prosemirror/src/index.ts"
      ],
      "@qti-components/test": [
        "./packages/qti-test/src/index.ts"
      ],
      "@qti-components/transformers": [
        "./packages/qti-transformers/src/index.ts"
      ],
      "@qti-components/loader": [
        "./packages/qti-loader/src/index.ts"
      ],
      "@qti-components/utilities": [
        "packages/lit-utilities/src/index.ts"
      ]
    }
  },
  "types": [
    "@types/dom-view-transitions",
    "./tools/storybook/vite-env.d.ts",
    "vitest/globals"
  ],
  "exclude": [
    "**/dist/**/*",
    "**/node_modules/**/*"
  ],
  "references": []
}